<script type="text/javascript">

	function initialize()
	{
		var mapContainer
		= document.getElementById("map");			//-- 지도를 표시할 div
		
		var mapOption =
		{
			center: new daum.maps.LatLng(37.556583, 126.919521)		//-- 지도의 중심좌표(풍성빌딩)	
			, level: 3												//-- 지도의 확대 레벨
		}
		
		// 지도를 표시할 div와 구성한 지도 옵션으로 지도 생성
		var map = new daum.maps.Map(mapContainer, mapOption);
	}
	
	
</script>

